{
  "/api/*": {
    "target": "http://localhost:8081",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": "/api"
    }
  },
  "/fans/*": {
    "target": "http://localhost:8081",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/fans": "/fans"
    }
  }
}
